{
  "uid": "manticore-buddy-prometheus",
  "title": "Manticore Buddy (Prometheus)",
  "tags": [
    "manticore",
    "buddy",
    "prometheus"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h"
    ]
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "current": {
          "selected": false,
          "text": "Prometheus",
          "value": "Prometheus"
        }
      },
      {
        "name": "job",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "$datasource"
        },
        "query": "label_values(manticore_uptime_seconds, job)",
        "refresh": 1,
        "current": {
          "selected": false,
          "text": "manticoresearch",
          "value": "manticoresearch"
        }
      },
      {
        "name": "instance",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "$datasource"
        },
        "query": "label_values(manticore_uptime_seconds{job=\"$job\"}, instance)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*",
        "multi": true,
        "current": {
          "selected": false,
          "text": "All",
          "value": ".*"
        }
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Uptime",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "max(manticore_uptime_seconds{job=\"$job\",instance=~\"$instance\"})",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Version",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "max by (version) (manticore_version{job=\"$job\",instance=~\"$instance\"})",
          "legendFormat": "{{version}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        },
        "textMode": "name"
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Current Connections",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "max(manticore_current_connections_count{job=\"$job\",instance=~\"$instance\"})",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Active Workers",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
      "targets": [
        {
          "expr": "max(manticore_workers_active_count{job=\"$job\",instance=~\"$instance\"})",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "values": false
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Load (All Queues)",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "gridPos": {"x": 0, "y": 4, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "manticore_load_1m_total{job=\"$job\",instance=~\"$instance\"}",
          "legendFormat": "1m",
          "refId": "A"
        },
        {
          "expr": "manticore_load_5m_total{job=\"$job\",instance=~\"$instance\"}",
          "legendFormat": "5m",
          "refId": "B"
        },
        {
          "expr": "manticore_load_15m_total{job=\"$job\",instance=~\"$instance\"}",
          "legendFormat": "15m",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 2
        },
        "overrides": []
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Work Queue Length",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "gridPos": {"x": 12, "y": 4, "w": 12, "h": 6},
      "targets": [
        {
          "expr": "manticore_work_queue_length_count{job=\"$job\",instance=~\"$instance\"}",
          "legendFormat": "queue",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Commands per Second",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "gridPos": {"x": 0, "y": 10, "w": 12, "h": 7},
      "targets": [
        {
          "expr": "sum by (instance) (rate(manticore_command_search_count{job=\"$job\",instance=~\"$instance\"}[5m]))",
          "legendFormat": "search",
          "refId": "A"
        },
        {
          "expr": "sum by (instance) (rate(manticore_command_insert_count{job=\"$job\",instance=~\"$instance\"}[5m]))",
          "legendFormat": "insert",
          "refId": "B"
        },
        {
          "expr": "sum by (instance) (rate(manticore_command_replace_count{job=\"$job\",instance=~\"$instance\"}[5m]))",
          "legendFormat": "replace",
          "refId": "C"
        },
        {
          "expr": "sum by (instance) (rate(manticore_command_update_count{job=\"$job\",instance=~\"$instance\"}[5m]))",
          "legendFormat": "update",
          "refId": "D"
        },
        {
          "expr": "sum by (instance) (rate(manticore_command_json_count{job=\"$job\",instance=~\"$instance\"}[5m]))",
          "legendFormat": "json",
          "refId": "E"
        },
        {
          "expr": "sum by (instance) (rate(manticore_command_sphinxql_count{job=\"$job\",instance=~\"$instance\"}[5m]))",
          "legendFormat": "sphinxql",
          "refId": "F"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2
        },
        "overrides": []
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Search Latency (p95/p99)",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "gridPos": {"x": 12, "y": 10, "w": 12, "h": 7},
      "targets": [
        {
          "expr": "manticore_search_stats_ms_pct95_5m_millisecond{job=\"$job\",instance=~\"$instance\"}",
          "legendFormat": "p95",
          "refId": "A"
        },
        {
          "expr": "manticore_search_stats_ms_pct99_5m_millisecond{job=\"$job\",instance=~\"$instance\"}",
          "legendFormat": "p99",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 2
        },
        "overrides": []
      }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "Query Cache Usage",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "gridPos": {"x": 0, "y": 17, "w": 12, "h": 7},
      "targets": [
        {
          "expr": "manticore_qcache_used_bytes{job=\"$job\",instance=~\"$instance\"}",
          "legendFormat": "used",
          "refId": "A"
        },
        {
          "expr": "manticore_qcache_max_bytes{job=\"$job\",instance=~\"$instance\"}",
          "legendFormat": "max",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "Query Cache Hit Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "gridPos": {"x": 12, "y": 17, "w": 12, "h": 7},
      "targets": [
        {
          "expr": "rate(manticore_qcache_hits_count{job=\"$job\",instance=~\"$instance\"}[5m])",
          "legendFormat": "hits/sec",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 2
        },
        "overrides": []
      }
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "Disk and RAM Bytes",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "gridPos": {"x": 0, "y": 24, "w": 12, "h": 7},
      "targets": [
        {
          "expr": "manticore_disk_bytes{job=\"$job\",instance=~\"$instance\"}",
          "legendFormat": "disk",
          "refId": "A"
        },
        {
          "expr": "manticore_ram_bytes{job=\"$job\",instance=~\"$instance\"}",
          "legendFormat": "ram",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "id": 12,
      "type": "timeseries",
      "title": "Disk Mapped Cache Ratio by Table",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "gridPos": {"x": 12, "y": 24, "w": 12, "h": 7},
      "targets": [
        {
          "expr": "manticore_disk_mapped_cached_ratio_percent{job=\"$job\",instance=~\"$instance\"}",
          "legendFormat": "{{table}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2
        },
        "overrides": []
      }
    },
    {
      "id": 13,
      "type": "timeseries",
      "title": "Slowest Thread",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "gridPos": {"x": 0, "y": 31, "w": 12, "h": 7},
      "targets": [
        {
          "expr": "manticore_slowest_thread_seconds{job=\"$job\",instance=~\"$instance\"}",
          "legendFormat": "seconds",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3
        },
        "overrides": []
      }
    },
    {
      "id": 14,
      "type": "timeseries",
      "title": "Connections by Type",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "gridPos": {"x": 12, "y": 31, "w": 12, "h": 7},
      "targets": [
        {
          "expr": "manticore_current_connections_count{job=\"$job\",instance=~\"$instance\"}",
          "legendFormat": "current",
          "refId": "A"
        },
        {
          "expr": "manticore_workers_clients_buddy_count{job=\"$job\",instance=~\"$instance\"}",
          "legendFormat": "buddy",
          "refId": "B"
        },
        {
          "expr": "manticore_workers_clients_vip_count{job=\"$job\",instance=~\"$instance\"}",
          "legendFormat": "vip",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      }
    }
  ]
}
